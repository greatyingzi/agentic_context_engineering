{
  "playbook_update_on_exit": true,
  "playbook_update_on_clear": true,
  "document_scanning_enabled": true,
  "git_scanning_enabled": true,
  "enable_spec_injection": true,
  "enable_spec_auto_generate": true,
  "spec_paths": [],
  "default_profile": "default",
  "global_fallback_profile": null,
  "spec_max_items": 6,
  "hooks": {
    "UserPromptSubmit": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "{{HOOK_COMMAND_USER_PROMPT_INJECT}}",
            "timeout": 120
          }
        ]
      }
    ],
    "SessionEnd": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "{{HOOK_COMMAND_SESSION_END}}",
            "timeout": 120
          }
        ]
      }
    ],
    "PreCompact": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "{{HOOK_COMMAND_PRECOMPACT}}",
            "timeout": 120
          }
        ]
      }
    ]
  }
}
